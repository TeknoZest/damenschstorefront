{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "node generate-locales.js && cross-env NODE_OPTIONS='--inspect' next dev",
    "dev-windows": "node generate-locales.js && cross-env NODE_OPTIONS='--inspect=5009' next dev",
    "build": "node generate-locales.js && next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "generate": "graphql-codegen",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js"
  },
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@react-spring/web": "^9.4.5",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.31.0",
    "@types/qs": "^6.9.7",
    "@types/react-gtm-module": "^2.0.1",
    "@types/store": "^2.0.2",
    "@vercel/fetch": "^6.2.0",
    "@xmldom/xmldom": "^0.8.2",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "email-validator": "^2.0.4",
    "formik": "^2.2.9",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.6.14",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "12",
    "next-seo": "^5.4.0",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.14",
    "postcss-nesting": "^10.1.7",
    "qs": "^6.10.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-fast-marquee": "^1.3.2",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-inner-image-zoom": "^3.0.0",
    "react-medium-image-zoom": "^4.4.3",
    "react-merge-refs": "^1.1.0",
    "react-paginate": "^8.1.3",
    "react-public-ip": "^1.0.0",
    "react-use-measure": "^2.1.1",
    "store": "^2.0.12",
    "stripe": "^9.5.0",
    "swell-js": "^4.0.0-next.0",
    "swiper": "^8.1.6",
    "swr": "^1.3.0",
    "tabbable": "^5.3.3",
    "tailwindcss": "^3.0.24",
    "uuidv4": "^6.2.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.random": "^3.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-inner-image-zoom": "^3.0.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.6.0",
    "prettier": "^2.6.2",
    "typescript": "4.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@assets/css/*": [
        "assets/css/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
